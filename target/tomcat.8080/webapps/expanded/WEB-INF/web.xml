<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>WebimTest</servlet-name>
		<servlet-class>webim.WebimTest</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WebimTest</servlet-name>
		<url-pattern>/webim_req</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Listener</servlet-name>
		<servlet-class>webim.Listener</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Listener</servlet-name>
		<url-pattern>/listener</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>OAuthCode</servlet-name>
		<jsp-file>/responseWithCode.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>OAuthCode</servlet-name>
		<url-pattern>/OAuthCode</url-pattern>
	</servlet-mapping>


	<display-name>Archetype Created Web Application</display-name>
</web-app>
